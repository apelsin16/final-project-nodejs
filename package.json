{
  "name": "goit-node-rest-api",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "start": "node ./app.js",
    "dev": "nodemon ./app.js",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "build": "npm run build-docs",
    "build-docs": "redocly bundle docs/openapi.yaml --ext json -o docs/swagger.json",
    "db:reset": "npx sequelize-cli db:migrate:undo:all && npx sequelize-cli db:migrate && npx sequelize-cli db:seed:all",
    "preview-docs": "redocly preview-docs"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.18.2",
    "gravatar": "^1.8.2",
    "joi": "^17.11.0",
    "morgan": "^1.10.0",
    "multer": "^2.0.1",
    "nodemailer": "^7.0.3",
    "pg": "^8.16.0",
    "sequelize": "^6.37.7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^11.1.0"
  },
  "devDependencies": {
    "@redocly/cli": "^1.34.4",
    "bcryptjs": "^3.0.2",
    "jest": "^29.7.0",
    "jsonwebtoken": "^9.0.2",
    "node-mocks-http": "^1.17.2",
    "nodemon": "^3.0.2",
    "sequelize-cli": "^6.6.3"
  }
}